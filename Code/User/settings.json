{
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontSize": 16,
    "editor.fontFamily": "'Iosevka SS10 Medium'",
    "workbench.colorTheme": "Dracula",
    "files.watcherExclude": {
        "**/target/**": true
    },
    "editor.lineNumbers": "off",
    "window.zoomLevel": 1,
    "editor.fontSize": 15,
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "breadcrumbs.enabled": true,
    "vim.leader": ",",
    "vim.insertModeKeyBindings": [
        { "before": ["j", "k"], "after": ["<Esc>"] },
        { "before": ["k", "j"], "after": ["<Esc>"] }
    ],
    "vim.normalModeKeyBindings": [
        { "before": ["H"], "after": ["^"] },
        { "before": ["L"], "after": ["$"] },
        { "before": ["Space"], "after": ["O", "<Esc>"] },
        { "before": ["Enter"], "after": ["o", "<Esc>"] }
    ],
    "editor.renderLineHighlight": "none",
    "editor.renderIndentGuides": false,
    "rust.clippy_preference": "on",
    "files.autoSave": "afterDelay",
    "lldb.executable": "/usr/bin/lldb",
    "gitlens.views.repositories.location": "scm",
    "gitlens.views.fileHistory.location": "scm",
    "gitlens.views.lineHistory.location": "scm",
    "gitlens.views.compare.location": "scm",
    "gitlens.views.search.location": "scm",
    "gitlens.blame.ignoreWhitespace": true,
    "lldb.adapterType": "classic",
    "editor.minimap.enabled": false,
    "rust-analyzer.enableEnhancedTyping": false,
    "scm.diffDecorations": "none"
}